{
  "name": "simple-server",
  "version": "1.0.0",
  "scripts": {
    "build:routes": "tsoa routes",
    "build:swagger": "tsoa spec",
    "build:all": "npm run build:routes && npm run build:swagger && npm run build",
    "start": "node dist/src/index.js",
    "build": "tsoa spec-and-routes && tsc",
    "dev": "concurrently \"nodemon\" \"nodemon --delay 800ms -x tsoa spec-and-routes\"",
    "lint:fix": "eslint ./src/ --fix",
    "prisma:db:push": "npx prisma db push",
    "prisma:studio": "npx prisma studio -p 5556",
    "test": "vitest",
    "test:cucumber": "cucumber-js --publish-quiet",
    "kill-port": "npx kill-port 6001"
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "@types/multer": "^1.4.7",
    "axios": "^1.4.0",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.9.0",
    "helmet": "^7.0.0",
    "multer": "1.4.5-lts.1",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsoa": "^5.1.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^9.3.0",
    "@faker-js/faker": "^8.0.2",
    "@types/convict": "^6.1.3",
    "@types/cors": "^2.8.13",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.8",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.46.0",
    "nodemon": "^3.0.1",
    "pactum": "^3.4.1",
    "prettier": "^3.0.1",
    "prisma": "^5.1.1",
    "typescript": "^5.1.6",
    "vitest": "^0.34.1",
    "vitest-mock-extended": "^1.2.0"
  }
}
